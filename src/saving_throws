      case 39: return  5;
      case 40: return  4;
      default:
	log("SYSERR: Missing level for warrior rod saving throw.");
      }
    case SAVING_PETRI:	/* Petrification */
      switch (level) {
      case  0: return 90;
      case  1: return 75;
